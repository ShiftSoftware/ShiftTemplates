@page "/"
@attribute [Authorize]
@using StockPlusPlus.Web.Pages.FormTest
@inject IDialogService DialogService

<MudStack Row Justify="Justify.SpaceBetween" AlignItems="AlignItems.Start" Reverse>
    <DateBig/>
</MudStack>

<MudButton OnClick="@(() => DialogService.Show<PerfTestForm>())">FormTest Dialog</MudButton>
<MudButton OnClick="@(() => DialogService.Show<HTMLForm>())">HTMLForm Dialog</MudButton>
<MudButton Href="/PerfTestForm">FormTest Page</MudButton>
<MudButton Href="/PerfTest2Form">FormTest 2 Page</MudButton>
<MudButton Href="/HTMLForm">HTMLForm Page</MudButton>

<MudAlert Class="mt-5" Variant="Variant.Text" Severity="Severity.Error" NoIcon>
    <MudText>
        <strong>A Very Unresponsive Form</strong>
    </MudText>

    <MudButton Class="mt-2" Variant="Variant.Outlined" Color="Color.Error" Href="/WarrantyClaimForm">Open The Form</MudButton>
</MudAlert>