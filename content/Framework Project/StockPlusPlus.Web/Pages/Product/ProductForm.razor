@attribute [Route($"/{nameof(ProductForm)}/{{Key?}}")]

<StockPlusPlus.WebComponents.Pages.Product.ProductForm Key="@Key" ProductBrandID="@ProductBrandID" ProductCategoryID="@ProductCategoryID"/>


@code {
    [Parameter]
    public string? Key { get; set; }

    [Parameter]
    public string? ProductCategoryID { get; set; }

    [Parameter]
    public string? ProductBrandID { get; set; }
}