@attribute [Route($"/filemanager/{{Root?}}")]

@if (Root is not null && InitialPath is not null)
{
    <FileExplorer FileManagerRoot="@Root" InitialPath="@InitialPath"></FileExplorer>
}


@code {
    [Parameter]
    public string Root { get; set; } = default!;

    [SupplyParameterFromQuery]
    public string InitialPath { get; set; } = default!;
}